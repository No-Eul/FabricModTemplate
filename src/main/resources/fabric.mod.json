{
	"schemaVersion": 1,
	"id": "template",
	"version": "${version}",
	"name": "${name}",
	"description": "This is an example description! Tell everyone what your mod is about!",
	"authors": [
		{
			"name": "NoEul",
			"contact": {
				"email": "me@NoEul.dev",
				"homepage": "https://NoEul.dev",
				"sources": "https://github.com/No-Eul"
			}
		}
	],
	"contact": {
		"email": "me@NoEul.dev",
		"homepage": "https://modrinth.com/mod/fabric-mod-template",
		"issues": "https://github.com/No-Eul/FabricModTemplate/issues",
		"sources": "https://github.com/No-Eul/FabricModTemplate"
	},
	"contributors": [],
	"license": "MIT",
	"icon": "assets/template/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": ["dev.noeul.fabricmod.template.FabricModTemplate"]
	},
	"mixins": [
		"template.mixins.json"
	],
	"depends": {
		"minecraft": "${minecraft_version}",
		"fabricloader": ">=${loader_version}",
		"fabric": ">=${fabric_api_version}"
	},
	"recommends": {
		"modmenu": "*"
	},
	"custom": {
		"modmenu": {
			"update_checker": true
		}
	}
}
